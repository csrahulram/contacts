        "experimentalDecorators": true,
        "outDir": "ng/dist",
        "sourceMap": true,
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "lib": [
            "dom",
            "es6"
